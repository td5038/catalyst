LABEL=OVERLAY		/overlay	ext4		noatime			0 2
LABEL=HOME		/home		ext4		noatime			0 2

overlay			/var		overlay		lowerdir=/var,upperdir=/overlay/var,workdir=/overlay/varw	0 0
overlay			/etc		overlay		lowerdir=/etc,upperdir=/overlay/etc,workdir=/overlay/etcw	0 0
overlay			/usr		overlay		lowerdir=/usr,upperdir=/overlay/usr,workdir=/overlay/usrw,noauto	0 0
